{
  "name": "va-admin-tabs",
  "private": false,
  "version": "0.0.3",
  "description": "a tab component of admin template, support vue2 and vue3",
  "author": "BadassFree (lixiaowen207@gmail.com)",
  "license": "MIT",
  "homepage": "https://github.com/BadassFree/va-admin-tabs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BadassFree/va-admin-tabs.git"
  },
  "bugs": {
    "url": "https://github.com/BadassFree/va-admin-tabs/issues"
  },
  "keywords": [
    "admin",
    "tab",
    "admin-tab",
    "vue2",
    "vue3"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
