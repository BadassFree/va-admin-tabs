    "pub": "npm publish --access=public",
    "preview": "vite preview"
  },
  "dependencies": {
    "colord": "^2.9.3",
    "css-render": "^0.15.12",
    "vue-demi": "^0.13.11"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.4.6"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "cross-env": "^7.0.3",
    "typescript": "^4.9.3",
    "unocss": "^0.49.1",
    "unplugin-vue-define-options": "^1.2.0",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.2",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.11"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
